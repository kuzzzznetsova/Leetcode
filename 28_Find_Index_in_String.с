int strStr(char* haystack, char* needle) {
    int len1 = strlen(haystack);
    int len2 = strlen(needle);
    int index = -1;

    for (int i = 0; i <= len1 - len2; i++) {
        int k = 0;
        for (int j = 0; j < len2; j++) {
            if (haystack[i+j] == needle[j]) {
                k++;
            } else {
                break;
            }
        }
        
        if (k == len2) {
            index = i;
            break;
        }
    }
    
    return index;
}
